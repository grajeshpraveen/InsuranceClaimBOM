rule renewalPolicy {
   property ilog.rules.business_name = "renewalPolicy";
   property ilog.rules.package_name = "";
   property status = "new";
   when {
      com.insurance.New_Policy() from new_Policy;
      evaluate (variable1 : (new_Policy.renewalPolicy()));
      evaluate (variable1 != null);
   } then {
      new_policy_approved = variable1;
      ?context.updateContext();
   } else {
      new_policy_approved = "IN_ACTIVE";
      ?context.updateContext();
   }
}

